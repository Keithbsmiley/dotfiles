index=$(tmux display -p "TMUXPWD_#D" | tr -d "%")
__pwd=$(tmux show-environment | grep --color=never "$index=" | sed 's/^.*=//')
if [[ -z "$__pwd" || ! -d "$__pwd" ]];then
  if [[ -f "$CURRENT_PROJECT_PATH" ]]; then
    __pwd="$(cat "$CURRENT_PROJECT_PATH")"
  else
    __pwd=$HOME
  fi
fi
tmux "$1" "$2" -c "$__pwd"
